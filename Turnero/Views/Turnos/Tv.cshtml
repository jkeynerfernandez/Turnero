<div class="container containerLogin">
    <table class="table table-striped table-bordered tablaTv">
        <thead>
            <tr>
                <th class="thTv">Puesto</th>
                <th class="thTv">Turno</th>
            </tr>
        </thead>
        <tbody>
            @foreach(var t in Model){
                <tr>
                    <td class="tdTv tdPuesto">@t.puesto</td>
                    <td class="tdTv">@t.TurnoId</td>
                </tr>
            }
        </tbody>
    </table>
</div>